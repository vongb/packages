{
  "name": "demo-get-parity",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/demo-get-parity/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/demo-get-parity",
        "main": "packages/demo-get-parity/src/index.ts",
        "tsConfig": "packages/demo-get-parity/tsconfig.lib.json",
        "assets": ["packages/demo-get-parity/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/demo-get-parity/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/demo-get-parity"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/demo-get-parity"
      }
    }
  },
  "tags": []
}
